<!DOCTYPE html>
<html>
    <head>
        <script src="https://aframe.io/releases/1.7.0/aframe.min.js"></script>
 
    </head>
    <body>
      
       
       <a-scene>
         <a-camera><a-cursor></a-cursor></a-camera>
        <a-sphere
            position="0 0 -5"
            depth="2"
            height="2"
            width="2"
            animation="
                property: position;
                from: 0 0 -5;
                to: 0 -8 -5;
                dur: 500;
                easing: linear;
                loop: true;
                dir:alternate;
                "
               animation__2="
               property:rotation;
               from: 0 0 0;
               to: 0 90 180;
               loop:true;
               dir:alternate;
              easeinout:elastic;" 
              
              animation__3="
              property:scale;
              from:1;
              to:0;
              startEvents:click;"
                
            src="https://codehs.com/uploads/c75f52a27709b7f762cca99e16d9f509">
        </a-sphere>
        <a-sky color="gray"></a-sky>
        <a-box position="0 1 -5" depth="2" height="4" width="0.5" color="green"
        animation="
        property: position;
        from:0 1 -10;
        to: 0 -7 -10;
        loop: true;
        dir:alternate;
        easinginout:quintic;"
     animation__3="
              property:scale;
              from:1;
              to:0;
              startEvents:click;"></a-box>
              <a-cone color="blue" radius-bottom="2" radius-top="0" height="5" position="-5 -8 -3"
              animation="
              property:position;
              from:-5 -7 -3;
              to: -5 0 -3;
              loop:true;
              dir:alternate;
              easingin:elastic;"
              
              animation__2="
              property:rotation;
              from: 0 0 0;
              to: 180 0 0;
              loop:true;
              dir:alternate;
              easingin:elastic;
              "
                animation__3="
              property:scale;
              from:1;
              to:0;
              startEvents:click;"></a-cone>

          <a-plane position="0 -10 0" rotation="-90 0 -5" width="160" height="90" src="https://codehs.com/uploads/637051be09bfbc2344739dc27580f594"></a-plane>
        </a-scene>
    </body>
</html>
